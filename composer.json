{
    "name": "xpressengine/laravel-cubrid",
    "description": "The cubrid database connetor for laravel",
    "keywords": ["laravel", "database", "db", "cubrid", "xpressengine"],
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Skriptex",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "5.7.*",
        "illuminate/support": "5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Xpressengine\\Cubrid\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Xpressengine\\Cubrid\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "illuminate/framework": "5.7.*"
    }
}
